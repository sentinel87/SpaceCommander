  {
    selectionFrame=1;
  }
  else
  {
    selectionFrame=0;
  }
  
  int fixedX=posX;
  int fixedY=posY;
  if(posX+34>80)
  {
    fixedX-=34;
  }
  if(posY+26>64)
  {
    fixedY-=26;
  }
  gb.display.setColor(BEIGE);
